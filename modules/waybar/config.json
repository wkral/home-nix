{
    "layer": "top",
    "height": 19,
    "modules-left": ["sway/workspaces", "sway/mode"],
    "modules-center": ["sway/window"],
    "modules-right": [
        "pulseaudio",
        "memory",
        "cpu",
        "battery",
        "network",
        "clock"
    ],
    "sway/window": {
        "max-length": 50
    },
    "sway/workspaces": {
        "disable-scroll": true,
        "format": "{icon} {name}",
        "format-icons": {
            "1": "",
            "urgent": "",
            "default": ""
        }
    },
    "battery": {
        "format": "{icon}",
        "format-icons": ["", "", "", "", ""],
        "states": {
          "percent100": 100,
          "percent95": 95,
          "percent90": 90,
          "percent85": 85,
          "percent80": 80,
          "percent75": 75,
          "percent70": 70,
          "percent65": 65,
          "percent60": 60,
          "percent55": 55,
          "percent50": 50,
          "percent45": 45,
          "percent40": 40,
          "percent35": 35,
          "percent30": 30,
          "percent25": 25,
          "percent20": 20,
          "percent15": 15,
          "percent10": 10,
          "percent5": 5,
          "percent0": 0
        }
    },
    "clock": {
        "format": " {:%a, %b %d %H:%M}"
    },
    "cpu": {
        "format": "",
        "max-length": 10,
        "interval": 5,
        "states": {
          "percent100": 100,
          "percent95": 95,
          "percent90": 90,
          "percent85": 85,
          "percent80": 80,
          "percent75": 75,
          "percent70": 70,
          "percent65": 65,
          "percent60": 60,
          "percent55": 55,
          "percent50": 50,
          "percent45": 45,
          "percent40": 40,
          "percent35": 35,
          "percent30": 30,
          "percent25": 25,
          "percent20": 20,
          "percent15": 15,
          "percent10": 10,
          "percent5": 5,
          "percent1": 1,
          "percent0": 0
        }
    },
    "memory": {
        "format": "",
        "states": {
          "percent100": 100,
          "percent95": 95,
          "percent90": 90,
          "percent85": 85,
          "percent80": 80,
          "percent75": 75,
          "percent70": 70,
          "percent65": 65,
          "percent60": 60,
          "percent55": 55,
          "percent50": 50,
          "percent45": 45,
          "percent40": 40,
          "percent35": 35,
          "percent30": 30,
          "percent25": 25,
          "percent20": 20,
          "percent15": 15,
          "percent10": 10,
          "percent5": 5,
          "percent0": 0
        }
    },
    "network": {
        "format-wifi": " {essid}",
        "format-ethernet": "{ifname} ",
        "format-disconnected": "",
        "max-length": 50
    },
    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-bluetooth": "{volume}% {icon}",
        "format-muted": "",
        "format-icons": {
            "headphones": "",
            "handsfree": "",
            "headset": "",
            "phone": "",
            "portable": "",
            "car": "",
            "default": ["", ""]
        }
    }
}
